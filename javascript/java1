<script>
var a = parseInt(prompt('Podaj bok a: '));
var b = parseInt(prompt('Podaj bok b: '));
var c = parseInt(prompt('Podaj bok c: '));
document.write(a + b + c);
if (a+b>c && a+c>b && b+c>a) {
	window.alert('Da sie');
} else {
	window.alert('Nie da sie');
}
</script>


